{
  "name": "port-ingress",
  "displayName": "PortIngress",
  "description": "Forward the port to the specified domain name.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "panel": [
        {
          "id": "portForwardPanel",
          "title": "PortIngress",
          "icon": ""
        }
      ]
    },
    "views": {
      "portForwardPanel": [
        {
          "id": "portForwardBottomView",
          "name": "Ports",
          "type": "webview",
          "icon": ""
        }
